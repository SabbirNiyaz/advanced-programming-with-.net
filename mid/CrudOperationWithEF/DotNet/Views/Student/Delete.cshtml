@{
    ViewBag.Title = "Delete";
}

@*student*@
<h4>Student ID : @Model.StudentID</h4>
<h4>Student Name : @Model.StudentName</h4>
<h4>Student Email : @Model.StudentEmail</h4>
<br />
<h2>Are you sure to delete this data?</h2>
<form method="post">
    <button type="submit" class="btn btn-danger">Yes</button>
    <a href="/Student/List" class="btn btn-secondary">No</a>
</form>